{
    "name": "yukata-roommate/laravel-validation",
    "description": "Validation for Laravel",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "YukataRm\\Laravel\\Validation\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YukataRm\\Laravel\\Validation\\Provider\\FacadeServiceProvider"
            ],
            "aliases": {
                "ValidationRules": "YukataRm\\Laravel\\Validation\\Facade\\ValidationRules"
            }
        }
    },
    "authors": [
        {
            "name": "Yukata Roommate",
            "email": "contact@yukata-roommate.com"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "laravel/framework": "^11.0",
        "yukata-roommate/laravel-provider": "^1.0.0"
    },
    "minimum-stability": "dev"
}